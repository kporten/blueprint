{
  "$schema": "./node_modules/turbo/schema.json",
  "ui": "tui",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"],
      "inputs": ["**/*.ts", "**/*.tsx"]
    },
    "test": {
      "inputs": ["**/*.ts", "**/*.tsx"]
    },
    "test:watch": {
      "cache": false,
      "interactive": true
    },
    "check": {
      "dependsOn": ["^check"],
      "inputs": ["**/*.ts", "**/*.tsx", "**/*.json"]
    },
    "check:fix": {
      "dependsOn": ["^check:fix"],
      "inputs": ["**/*.ts", "**/*.tsx", "**/*.json"]
    },
    "db:generate": {
      "cache": false,
      "interactive": true
    },
    "db:migrate": {
      "cache": false
    },
    "db:check": {
      "cache": false
    },
    "db:push": {
      "cache": false,
      "interactive": true
    },
    "db:seed": {
      "cache": false
    }
  }
}
