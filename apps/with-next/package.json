{
  "name": "@blueprint/with-next",
  "description": "This is a blueprint for a web app with Next.js.",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "bunx --bun next dev",
    "build": "bunx --bun next build",
    "typecheck": "bunx --bun tsc",
    "check": "bunx --bun biome check --error-on-warnings",
    "check:fix": "bunx --bun biome check --fix",
    "db:generate": "bunx --bun drizzle-kit generate",
    "db:migrate": "bunx --bun drizzle-kit migrate",
    "db:check": "bunx --bun drizzle-kit check",
    "db:push": "bunx --bun drizzle-kit push",
    "db:seed": "bun run ./src/db/seed.ts"
  },
  "dependencies": {
    "@neondatabase/serverless": "1.0.2",
    "@nosecone/next": "1.0.0-beta.14",
    "@t3-oss/env-nextjs": "0.13.8",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "drizzle-orm": "0.44.7",
    "lucide-react": "0.553.0",
    "next": "16.0.1",
    "next-intl": "4.5.0",
    "nuqs": "2.7.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "3.4.0",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.17",
    "@total-typescript/tsconfig": "1.0.4",
    "@types/bun": "1.3.2",
    "@types/node": "24.10.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "drizzle-kit": "0.31.6",
    "drizzle-seed": "0.3.1",
    "postcss": "8.5.6",
    "postgres": "3.4.7",
    "tailwindcss": "4.1.17",
    "tw-animate-css": "1.4.0"
  }
}
